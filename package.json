{
  "name": "praxisplaner",
  "version": "0.0.1",
  "private": true,
  "description": "TBD",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "pnpm exec concurrently -r pnpm:dev:web pnpm:dev:db",
    "dev:db": "pnpm exec convex dev",
    "dev:web": "pnpm exec wait-on tcp:127.0.0.1:3210 -t 60000 && vite dev",
    "format": "prettier --write .",
    "format:list": "prettier --list-different .",
    "gen": "pnpm exec concurrently -r pnpm:gen:routes pnpm:gen:convex",
    "gen:convex": "pnpm exec convex codegen",
    "gen:routes": "pnpm exec tsr generate",
    "lint": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "start": "vite start",
    "test": "vitest --run"
  },
  "dependencies": {
    "@convex-dev/react-query": "0.0.0-alpha.8",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-query-devtools": "^5.80.7",
    "@tanstack/react-router": "^1.121.2",
    "@tanstack/react-router-devtools": "^1.121.8",
    "@tanstack/react-router-with-query": "^1.121.2",
    "@tanstack/react-start": "^1.121.7",
    "@tanstack/react-start-plugin": "^1.121.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.24.8",
    "date-fns": "^4.1.0",
    "idb-keyval": "^6.2.2",
    "ky": "^1.8.1",
    "lucide-react": "^0.513.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-day-picker": "9.7.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "redaxios": "^0.5.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "tiny-invariant": "^1.3.3",
    "tw-animate-css": "^1.3.4",
    "vite": "^6.3.5",

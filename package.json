{
  "name": "praxisplaner",
  "version": "0.0.1",
  "private": true,
  "description": "TBD",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:vercel": "if [ \"$VERCEL_ENV\" = \"preview\" ]; then PREVIEW_NAME=\"$(printf '%s' \"$VERCEL_GIT_COMMIT_REF\" | tr '/' '-')\"; npx convex deploy --cmd \"npm run build\" --preview-create \"$PREVIEW_NAME\" && npx convex import --preview-name \"$PREVIEW_NAME\" --replace-all seed_data.zip; else npx convex deploy --cmd \"npm run build\"; fi",
    "dev": "pnpm dev:frontend & pnpm dev:backend",
    "dev:backend": "pnpm exec convex dev",
    "dev:frontend": "vite dev",
    "format": "prettier --write .",
    "format:list": "prettier --list-different .",
    "gen": "pnpm gen:routes & pnpm gen:convex",
    "gen:convex": "pnpm exec convex codegen",
    "gen:routes": "pnpm exec tsr generate",
    "lint": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "start": "vite start",
    "test": "VITE_ENABLE_POSTHOG_IN_DEV=true vitest --run"
  },

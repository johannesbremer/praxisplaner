{
  "name": "praxisplaner",
  "version": "0.0.1",
  "private": true,
  "description": "TBD",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "vinxi build",
    "dev": "pnpm exec concurrently -r pnpm:dev:web pnpm:dev:db",
    "dev:db": "pnpm exec convex dev",
    "dev:web": "pnpm exec wait-on tcp:127.0.0.1:3210 -t 60000 && vinxi dev",
    "format": "prettier --write .",
    "format:list": "prettier --list-different .",
    "gen": "pnpm exec concurrently -r pnpm:gen:routes pnpm:gen:convex",
    "gen:convex": "pnpm exec convex codegen",
    "gen:routes": "pnpm exec tsr generate",
    "lint": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "start": "vinxi start",
    "test": "vitest"
  },
  "dependencies": {
    "@convex-dev/react-query": "0.0.0-alpha.8",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.76.2",
    "@tanstack/react-query-devtools": "^5.76.2",
    "@tanstack/react-router": "^1.120.7",
    "@tanstack/react-router-devtools": "^1.120.7",
    "@tanstack/react-router-with-query": "^1.120.7",
    "@tanstack/react-start": "^1.120.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.24.1",
    "idb-keyval": "^6.2.2",
    "ky": "^1.8.1",
    "lucide-react": "^0.511.0",
    "msw": "^2.8.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "redaxios": "^0.5.1",
    "tailwind-merge": "^3.3.0",
    "tiny-invariant": "^1.3.3",
    "tw-animate-css": "^1.3.0",
    "vinxi": "0.5.6",
    "zod": "^3.25.23"
  },
  "devDependencies": {
    "@convex-dev/eslint-plugin": "0.0.1-alpha.4",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.27.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@tanstack/eslint-plugin-router": "^1.115.0",
    "@tanstack/router-cli": "^1.120.7",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/eslint-plugin": "^1.2.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-package-json": "^0.31.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-yml": "^1.18.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4",
    "wait-on": "^8.0.3"
  },
  "packageManager": "pnpm@10.11.0"
}

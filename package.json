{
  "name": "praxisplaner",
  "version": "0.0.1",
  "private": true,
  "description": "TBD",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "pnpm gen && vite build",
    "dev": "pnpm exec concurrently -r pnpm:dev:web pnpm:dev:db",
    "dev:db": "pnpm exec convex dev",
    "dev:web": "pnpm exec wait-on tcp:127.0.0.1:3210 -t 60000 && vite dev",
    "format": "prettier --write .",
    "format:list": "prettier --list-different .",
    "gen": "pnpm exec concurrently -r pnpm:gen:routes pnpm:gen:convex",
    "gen:convex": "pnpm exec convex codegen",
    "gen:routes": "pnpm exec tsr generate",
    "lint": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "start": "vite start",
    "test": "VITE_ENABLE_POSTHOG_IN_DEV=true vitest --run",
    "test-build": "vite build"
  },
